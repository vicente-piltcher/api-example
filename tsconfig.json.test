{
    "compilerOptions": {
      "target": "ES6",
      "module": "ES6",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "strict": true,
      "outDir": "./dist",
      "rootDir": "./src",
      "resolveJsonModule": true,
      "allowSyntheticDefaultImports": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "dist"]
  }